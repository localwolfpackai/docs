{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "humanlup docs",
  "colors": {
    "primary": "#64748B",
    "light": "#94A3B8",
    "dark": "#475569"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Toolkit",
        "groups": [
          {
            "group": "Start Here",
            "pages": [
              "index",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Essentials",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
            ]
          },
          {
            "group": "Going Deeper",
            "pages": [
              "advanced/components",
              "advanced/seo-metadata",
              "advanced/versioning"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "api-reference/introduction",
              "api-reference/authentication"
            ]
          },
          {
            "group": "Endpoints",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/localwolfpackai",
          "icon": "github"
        },
        {
          "anchor": "Skills.sh",
          "href": "https://skills.sh",
          "icon": "bolt"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "GitHub",
        "href": "https://github.com/localwolfpackai"
      }
    ],
    "primary": {
      "type": "github",
      "label": "GitHub",
      "href": "https://github.com/localwolfpackai/docs"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/localwolfpackai"
    }
  },
  "feedback": {
    "suggestEdit": true,
    "raiseIssue": true
  },
  "search": {
    "prompt": "Search my docs..."
  },
  "openapi": ["/api-reference/openapi.json"]
}
